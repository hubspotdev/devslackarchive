  {
    "poster": {
      "poster_name": "Gregory Cornelius",
      "poster_handle": "gcornelius"
    },
    "time_stamp": "2017-02-03 12:13:07",
    "text": "you can append `?hsDebug=true` to the url when loading the page"
  },
  {
    "poster": {
      "poster_name": "Nicholas Decker - Level -22",
      "poster_handle": "nickdeckerdevs"
    },
    "time_stamp": "2017-02-03 12:13:42",
    "text": "thanks"
